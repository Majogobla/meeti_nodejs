<div class="contenedor">
    <h1><%= nombrePagina %></h1>

    <% if(meetis.length) { %>
        <div class="grid columnas-md-3 ">
            <% meetis.forEach(meeti => { %>
                <%- include('partials/cardMeeti.ejs', {meeti}); %>
            <% }) %>
        </div> <!--.grid-->
    <% } else { %>
        <p>No hay resultados</p>
    <% } %>
</div>